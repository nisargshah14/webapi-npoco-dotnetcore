{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server={Server};Database={Db};User Id={userid};Password={password};Trusted_Connection=true;"
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "PageSize": "50"
  }
}
